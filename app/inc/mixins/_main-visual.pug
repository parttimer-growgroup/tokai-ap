//- メインビジュアル
mixin c_main-visual
  +c.main-visual
    +e.slider.owl-carousel
      +bgimg("img-mainv-01.jpg").c-main-visual__image
      +bgimg("img-mainv-02.jpg").c-main-visual__image
      +bgimg("img-mainv-03.jpg").c-main-visual__image
    +e.inner
      +e.title: +bgimg("img-mainv-title.png").c-main-visual__title-img
